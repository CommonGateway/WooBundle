{
  "title": "Bijlage",
  "$id": "https://commongateway.nl/woo.bijlage.schema.json",
  "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
  "version": "0.0.5",
  "type": "object",
  "description": "",
  "required": ["titel"],
  "properties": {
    "id": {
      "type": "string",
      "description": "identificatienummer"
    },
    "titel": {
      "type": "string",
      "required": true,
      "description": "Titel van deze bijlage"
    },
    "url": {
      "type": "string",
      "format": "url",
      "description": "De URL voor het bekijken van deze bijlage; dit zou een link moeten zijn die, wanneer geopend in een browser, de bijlage downloadt of weergeeft."
    },
    "status": {
      "type": "string",
      "description": "Status van deze bijlage"
    },
    "type": {
      "type": "string",
      "description": "mime-type van deze bijlage oftewel bestandsextensie bijv (.pdf)"
    },
    "categorie": {
      "type": "string",
      "description": "Categorie van deze bijlage"
    }
  }
}